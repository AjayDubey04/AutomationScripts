package deltabatch123456;

public class Hello {

	public static void main(String []args)
	{
		int i,j,n,k,a;
		for(i=6; i>=1; i-- )      
		{
			for( j=1; j<=i; j++ ) 
				System.out.print(" <*> ");
			System.out.println();
		}
		System.out.println(" |*|"+" Ajay "+" |*|"+" Dubey"+" |*|");
	for(n=1; n<=6; n++ )      
	{
		for( k=1; k<=n; k++ ) 
			System.out.print(" <*> ");
		System.out.println();
	}
	for(a=5; a<=1; i++ )      
	System.out.print("                           <*>");
	}
}