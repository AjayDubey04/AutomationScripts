package Practice;

public class ReversString {

	
	public static void main(String[] args) {
		
		first();
		
	}
	
	public static void first() {
		
		String str="Hello";
//		System.out.println(str.charAt(0));
		
		for(int i=str.length()-1; i>=0; i--) {
			System.out.println(str.charAt(i));
			
		}
		
		
		
		
	}
	
}
