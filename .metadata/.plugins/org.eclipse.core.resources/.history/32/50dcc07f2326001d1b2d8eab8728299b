package deltabatch123456;

public class Hello {

	public static void main(String []args)
	{
		int i,j,k;
		for(i=1; i<=5; i++ )      
		{
			for( j=1; j<=i; j++ ) 
			{
				System.out.print(" *");
			}	System.out.println();
		
		}
		int n;
		for(k=5; k>=1; k-- )      
		{
			for( n=1; n<=i; n++ ) 
			{
				System.out.print(" * ");
	}System.out.println();
}
	}
}