package RecapCoding;

public class Practice  extends Thread {



	public void fun() {

		for(int i=1; i<=3; i++) {
			System.out.println("Hello");
		}
	}




	public static void main(String[] args) {
	
		}
		
		

	}


}

class B {

	public static void main(String[] args) {

		System.out.println("Namaste India");
		Practice obj=new Practice();
		obj.start();
		obj.fun();
		for(int i=1; i<=3; i++) {
			System.out.println("Java");
		}

	}

}