package RecapCoding;

public class RecapeSwapping {

	public static void main(String[] args) {

		
			String str="I Am Going To Market";
			for (int i = 0; i <str.length(); i++) {
				
				for(char a=65; a<95; a++) {
					if(str.charAt(i)==a) {
						System.out.println(a);
					}
				}
				
			}
		
//		RecapeSwapping obj=new RecapeSwapping();
//	    obj.swap_1();
//	    System.out.println();
//		obj.swap_2();
		
	}
	
	
	public void swap_1() {
		
		int a=10;
		int b=20;
		int c;
		c=b;
		b=a;
		a=c;
		System.out.println("a = "+a);
		System.out.println("b = "+b);
	
		
	}
	
	public void swap_2() {
		
		int a=10;
		int b=20;
		a=a+b;
		b=a-b;
		a=a-b;
		
		System.out.println("a = "+a);
		System.out.println("b = "+b);
		
	}

}
