	public static void main(String[] args) 
	{
		 System.setProperty("webdriver.chrome.driver", "Driver\\chromedriver.exe");
		 ChromeDriver obj=new ChromeDriver();
		 obj.get("http://www.facebook.com");
		 
		 System.setProperty("webdriver.edge.driver", "Driver\\msedgedriver.exe"); 
		 EdgeDriver only=new EdgeDriver();
		 only.get("http://localhost:8888");
		 only.get("http://whatsapp.com");
		 only.get("http://gmail.com");
		WebElement hii= only.findElementByXPath("//input[@type=\"email\"]");
		hii.sendKeys("ajaydubeyji63@outlook.com");

	}	
