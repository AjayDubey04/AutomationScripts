package deltabatch123456;

public class Hello {

	public static void main(String []args)
	{
		int i,j,n,k;
		for(i=5; i>=1; i-- )      
		{
			for( j=1; j<=i; j++ ) 
				System.out.print(" *");
			System.out.println();
		}
		
	for(n=1; n<=5; n++ )      
	{
		for( k=1; k<=n; k++ ) 
			System.out.print(" *");
		System.out.println();
	}
	}
}