<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="SmokeTestSuite" parallel="tests" >

   <listeners>
        <listener class-name="com.vtiger.test.listners.ExtentReportListener" />
    </listeners>
 
	<test name="MarketingChromeTestCase">
		 <parameter name="Browser" value="chrome" />		
		<classes>
			<class
				name="com.example.testscripts.MarketingAccountTestScripts">
				<methods>
					<include name="verifyCreateAccountTC001" />
					<exclude name="verifySearchAccountTC002" />
					<exclude name="verifyDuplicateAccountTC003" />
					<exclude name="verifyAccountEditableTC004" />
					<exclude name="verifyDeleteAccountTC005" />
				</methods>
			</class>
		</classes>
	</test>
	
	
	
	<test name="MarketingEdgeTestCase">
	<parameter name="Browser" value="edge" />		
		<classes>
			<class
				name="com.example.testscripts.MarketingAccountTestScripts">
				<methods>
					<include name="verifyCreateAccountTC001" />
					<exclude name="verifySearchAccountTC002" />
					<exclude name="verifyDuplicateAccountTC003" />
					<exclude name="verifyAccountEditableTC004" />
					<exclude name="verifyDeleteAccountTC005" />
				</methods>
			</class>
		</classes>
	</test>
	

	
	<!-- <listeners>
        <listener class-name="com.vtiger.test.listners.RetryAnalyzer" />
    </listeners> -->
	
</suite> 
